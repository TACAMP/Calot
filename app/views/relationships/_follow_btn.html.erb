  <% if current_user != obj %>
    <% if current_user.following?(obj) %>
      <%= link_to 'フォローを外す' , user_relationships_path(obj.id) , method: :delete , remote: true %>
    <% else %>
      <%= link_to 'フォローする' , user_relationships_path(obj.id) , method: :post , remote: true %>
    <% end %>
  <% end %>